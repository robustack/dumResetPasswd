<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>resetPasswd</display-name>
 <servlet>
   <servlet-name>reset</servlet-name>
   <servlet-class>com.small.resetpasswd.account.resetPasswd</servlet-class>
 </servlet>
 <servlet-mapping>
   <servlet-name>reset</servlet-name>
   <url-pattern>/reset</url-pattern>
 </servlet-mapping>
  <servlet>
   <servlet-name>done</servlet-name>
   <servlet-class>com.small.resetpasswd.account.Done</servlet-class>
 </servlet>
 <servlet-mapping>
   <servlet-name>done</servlet-name>
   <url-pattern>/Done</url-pattern>
 </servlet-mapping>
  <welcome-file-list>
    <welcome-file>unknown-request.html</welcome-file>
  </welcome-file-list>
</web-app>
